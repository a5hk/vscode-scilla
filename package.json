{
  "name": "vscode-scilla",
  "displayName": "Scilla",
  "description": "Syntax highlighting for Scilla",
  "version": "0.0.2",
  "publisher": "a5hk",
  "author": "a5hk",
  "license": "MIT",
  "icon": "icon.png",
  "homepage": "https://github.com/a5hk/vscode-scilla",
  "repository": {
    "type": "git",
    "url": "https://github.com/a5hk/vscode-scilla"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "scilla",
        "aliases": [
          "Scilla",
          "scilla"
        ],
        "extensions": [
          ".scilla"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "scilla",
        "scopeName": "source.scilla",
        "path": "./syntaxes/scilla.tmLanguage.json"
      }
    ]
  }
}
